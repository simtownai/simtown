{
  "name": "simtown",
  "version": "0.0.1",
  "type": "module",
  "description": "",
  "scripts": {
    "dev": "concurrently \"bun run dev:server\" \"bun run dev:client\" \"bun run dev:npc\"",
    "dev:server": "bun run --watch src/server/server.ts",
    "dev:client": "vite --port 3001",
    "dev:npc": "bun run --watch src/npc/client.ts",
    "build": "vite build",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "axios": "^1.7.7",
    "easystarjs": "^0.4.4",
    "event-source-polyfill": "^1.0.31",
    "express": "^4.17.1",
    "marked": "^14.1.2",
    "openai": "^4.67.3",
    "phaser": "^3.85.2",
    "phaser3-rex-plugins": "^1.80.8",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "vite-plugin-svgr": "^4.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.12",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^7.0.0",
    "prettier": "^3.3.3",
    "typescript": "^4.5.4",
    "vite": "^5.4.8"
  }
}
